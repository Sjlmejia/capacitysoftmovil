<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/s" >

    <TextView
        android:id="@+id/lbl_hfip"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentRight="true"
        android:layout_centerVertical="true"
        android:layout_marginRight="88dp"
        android:text="Small Text"
        android:textSize="25sp"
        android:textAppearance="?android:attr/textAppearanceSmall"
        tools:ignore="HardcodedText" />

    <TextView
        android:id="@+id/lbl_hiniciop"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_above="@+id/lbl_hfip"
        android:layout_alignLeft="@+id/lbl_hfip"
        android:layout_marginBottom="14dp"
        android:text="Small Text"
        android:textSize="25sp"
        android:textAppearance="?android:attr/textAppearanceSmall"
        tools:ignore="HardcodedText" />

    <TextView
        android:id="@+id/lbl_finp"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_above="@+id/lbl_hiniciop"
        android:layout_alignLeft="@+id/lbl_hiniciop"
        android:layout_marginBottom="16dp"
        android:text="Small Text"
        android:textSize="25sp"
        android:textAppearance="?android:attr/textAppearanceSmall"
        tools:ignore="HardcodedText" />

    <TextView
        android:id="@+id/lbl_iniciop"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_above="@+id/lbl_finp"
        android:layout_alignLeft="@+id/lbl_finp"
        android:layout_marginBottom="14dp"
        android:text="Small Text"
        android:textSize="25sp"
        android:textAppearance="?android:attr/textAppearanceSmall"
        tools:ignore="HardcodedText" />

    <TextView
        android:id="@+id/lbl_lugarp"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_above="@+id/lbl_iniciop"
        android:layout_alignLeft="@+id/lbl_iniciop"
        android:layout_marginBottom="20dp"
        android:text="Small Text"
        android:textSize="25sp"
        android:textAppearance="?android:attr/textAppearanceSmall"
        tools:ignore="HardcodedText" />

    <TextView
        android:id="@+id/lbl_temap"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_above="@+id/lbl_lugarp"
        android:layout_alignLeft="@+id/lbl_lugarp"
        android:layout_marginBottom="14dp"
        android:text="Small Text"
        android:textSize="25sp"
        android:textAppearance="?android:attr/textAppearanceSmall"
        tools:ignore="HardcodedText" />

    <TextView
        android:id="@+id/lbl_nombreprueba"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentLeft="true"
        android:layout_alignTop="@+id/lbl_temap"
        android:text="Tema:"
        android:textSize="25sp"
        android:textAppearance="?android:attr/textAppearanceMedium"
        android:textColor="@color/tomate"
        tools:ignore="HardcodedText" />

    <TextView
        android:id="@+id/textView2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignBottom="@+id/lbl_lugarp"
        android:layout_alignParentLeft="true"
        android:text="Lugar: "
        android:textSize="25sp"
        android:textAppearance="?android:attr/textAppearanceMedium"
        android:textColor="@color/tomate"
        tools:ignore="HardcodedText" />

    <TextView
        android:id="@+id/textView3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignBottom="@+id/lbl_iniciop"
        android:layout_alignParentLeft="true"
        android:text="Fecha inicio:"
        android:textSize="25sp"
        android:textAppearance="?android:attr/textAppearanceMedium"
        android:textColor="@color/tomate"
        tools:ignore="HardcodedText" />

    <TextView
        android:id="@+id/textView4"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignBottom="@+id/lbl_finp"
        android:layout_alignParentLeft="true"
        android:text="Fecha fin:"
        android:textSize="25sp"
        android:textAppearance="?android:attr/textAppearanceMedium"
        android:textColor="@color/tomate"
        tools:ignore="HardcodedText" />

    <TextView
        android:id="@+id/textView5"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentLeft="true"
        android:layout_below="@+id/lbl_finp"
        android:text="Hora inicio:"
        android:textSize="25sp"
        android:textAppearance="?android:attr/textAppearanceMedium"
        android:textColor="@color/tomate"
        tools:ignore="HardcodedText" />

    <TextView
        android:id="@+id/textView6"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentLeft="true"
        android:layout_alignTop="@+id/lbl_hfip"
        android:text="Hora fin:"
        android:textSize="25sp"
        android:textAppearance="?android:attr/textAppearanceMedium"
        android:textColor="@color/tomate"
        tools:ignore="HardcodedText" />

    <Button
        android:id="@+id/btn_cancelari"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentRight="true"
        android:layout_alignParentTop="true"
        android:background="@drawable/botonpulsa"
        android:text="Atras"
        android:textColor="#EDE7F6"
        android:textSize="15sp"
        android:textStyle="bold"
        tools:ignore="HardcodedText" />

    <TextView
        android:id="@+id/lbl_calificacionh"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignBaseline="@+id/textView7"
        android:layout_alignBottom="@+id/textView7"
        android:layout_alignLeft="@+id/lbl_hfip"
        android:text="Small Text"
        android:textSize="25sp"
        android:textAppearance="?android:attr/textAppearanceSmall"
        tools:ignore="HardcodedText" />

    <TextView
        android:id="@+id/textView7"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentLeft="true"
        android:layout_below="@+id/textView6"
        android:layout_marginTop="20dp"
        android:text="Calificación"
        android:textSize="25sp"
        android:textAppearance="?android:attr/textAppearanceMedium"
        android:textColor="@color/tomate"
        tools:ignore="HardcodedText" />

    <TextView
        android:id="@+id/lbl_direccion"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentTop="true"
        android:layout_marginTop="21dp"
        android:layout_toRightOf="@+id/textView4"
        android:text="Información"
        android:textAppearance="?android:attr/textAppearanceLarge"
        android:textColor="#263238"
        android:textSize="40sp"
        android:textStyle="bold"
        tools:ignore="HardcodedText" />

</RelativeLayout>